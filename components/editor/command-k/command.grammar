@top command {
    AddFile | ClearOPFS
}


@skip { LineComment }

AddFile {
    "add-file" " "+ FileName
}

ClearOPFS {
    "clear-opfs"
}

FileName {
    identifier '.' identifier
}


@tokens {
  identifier { $[a-zA-Z0-9_]+ }
  // QuotedString { '"' (!["\\] | "\\" _)* '"' }
  LineComment { ";; " ![\n]* }
  // space { $[ \t\n\r]+ }
}

@detectDelim