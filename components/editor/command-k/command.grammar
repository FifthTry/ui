@top command {
    AddFile
}


@skip { space | LineComment }

AddFile {
    "add-file" FileName
}


FileName {
    identifier '.' identifier
}


@tokens {
  identifier { $[a-zA-Z0-9_]+ }
  // QuotedString { '"' (!["\\] | "\\" _)* '"' }
  LineComment { "-- " ![\n]* }
  space { $[ \t\n\r]+ }
}

@detectDelim