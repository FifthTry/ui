@top Command {
    AddFile
}


@skip { space | LineComment }

AddFile {
    "add-file" FileName
}


FileName {
    Identifier '.' Identifier
}


@tokens {
  Identifier { $[a-zA-Z0-9_]+ }
  // QuotedString { '"' (!["\\] | "\\" _)* '"' }
  LineComment { "-- " ![\n]* }
  space { $[ \t\n\r]+ }
}

@detectDelim